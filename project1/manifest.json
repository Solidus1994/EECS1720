{
  "manifest_version": 2,
  "name": "Project 1",
  "description": "This extension allows the user to change the background color of part of the current web page.",
  "version": "1.0",
  "icons": {
    "128": "icon.png",
    "48": "icon.png",
    "48": "icon.png",
    "16": "icon.png"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
